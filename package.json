{
   "name": "type-test",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.17.1",
      "@types/cors": "^2.8.6",
      "@types/express": "^4.17.1",
      "@types/helmet": "^0.0.45",
      "@types/jsonwebtoken": "^8.3.4",
      "@types/multer": "^1.3.10",
      "@types/node": "^10.14.22",
      "husky": "^3.0.9",
      "prettier": "^1.18.2",
      "pretty-quick": "^2.0.0",
      "ts-node": "8.4.1",
      "tslint": "^5.20.0",
      "tslint-config-airbnb": "^5.11.2",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "3.6.4"
   },
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "class-transformer": "^0.2.3",
      "class-validator": "^0.10.2",
      "class-validator-jsonschema": "^1.3.0",
      "cors": "^2.8.5",
      "express": "^4.15.4",
      "helmet": "^3.21.1",
      "jsonwebtoken": "^8.5.1",
      "multer": "^1.4.2",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "routing-controllers": "^0.8.0",
      "routing-controllers-openapi": "^1.7.0",
      "ts-node-dev": "^1.0.0-pre.43",
      "tsyringe": "^3.4.0",
      "typeorm": "^0.2.20"
   },
   "scripts": {
      "build": "tsc",
      "start": "set debug=* && ts-node-dev --respawn --transpileOnly ./src/index.ts",
      "prod": "tsc && node ./build/index.js",
      "add-migration": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
      "update-database": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
      "tslint": "tslint --project tsconfig.json",
      "prettier:base": "prettier --parser typescript --single-quote",
      "prettier:check": "yarn run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
      "prettier:write": "yarn run prettier:base -- --write \"src/**/*.{ts,tsx}\""
   },
   "husky": {
      "hooks": {
         "pre-commit": "pretty-quick --staged"
      }
   }
}
