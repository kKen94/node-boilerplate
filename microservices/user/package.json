{
   "name": "type-test",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.17.1",
      "@types/cors": "^2.8.6",
      "@types/express": "^4.17.1",
      "@types/helmet": "^0.0.45",
      "@types/jsonwebtoken": "^8.3.4",
      "@types/multer": "^1.3.10",
      "@commitlint/cli": "^8.2.0",
      "@commitlint/config-conventional": "^8.2.0",
      "@commitlint/prompt": "^8.2.0",
      "@types/node": "^12.12.14",
      "cross-env": "^6.0.3",
      "husky": "^4.0.9",
      "lint-staged": "^9.4.3",
      "prettier": "^1.18.2",
      "ts-node": "^8.5.0",
      "ts-node-dev": "^1.0.0-pre.43",
      "tsconfig-paths": "^3.9.0",
      "tslint": "^5.20.0",
      "tslint-config-airbnb": "^5.11.2",
      "tslint-config-prettier": "^1.18.0",
      "typeorm-seeding": "betiol/typeorm-seeding",
      "typescript": "^3.7.2"
   },
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "class-transformer": "^0.2.3",
      "class-validator": "^0.11.0",
      "class-validator-jsonschema": "^1.3.0",
      "cors": "^2.8.5",
      "express": "^4.15.4",
      "helmet": "^3.21.1",
      "jsonwebtoken": "^8.5.1",
      "morgan": "^1.9.1",
      "multer": "^1.4.2",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "rotating-file-stream": "^2.0.0",
      "routing-controllers": "^0.8.0",
      "routing-controllers-openapi": "^1.7.0",
      "swagger-stats": "^0.95.11",
      "swagger-ui-express": "^4.1.2",
      "tsyringe": "^4.0.1",
      "typeorm": "^0.2.20"
   },
   "scripts": {
      "build:staging": "cross-env NODE_ENV=staging tsc",
      "build:prod": "cross-env NODE_ENV=prod tsc",
      "build": "cross-env NODE_ENV=dev tsc",
      "start": "set debug=* && cross-env NODE_ENV=dev ts-node-dev --respawn --transpileOnly -r tsconfig-paths/register ./src/index.ts",
      "add-migration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
      "update-database": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
      "seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed --config ormconfig.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
      "tslint": "tslint --project tsconfig.json",
      "copy:files": "yarn run copy:readme && yarn run copy:conduct",
      "copy:readme": "copyfiles ./README.md ./dist/lib",
      "copy:conduct": "copyfiles ./CODE_OF_CONDUCT.md ./dist/lib",
      "format:check": "prettier --list-different \"src/**/*.{ts,tsx}\"",
      "format:write": "prettier --write \"src/**/*.{ts,tsx}\""
   },
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged",
         "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
      }
   },
   "commitlint": {
      "extends": [
         "@commitlint/config-conventional"
      ]
   },
   "lint-staged": {
      "src/**/*.ts": [
         "prettier --write",
         "git add"
      ]
   },
   "private": true,
   "repository": {
      "type": "git",
      "url": "https://github.com/kKen94/eagle-api.git"
   }
}
