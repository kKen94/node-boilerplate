{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-string-literal": false,
    "no-trailing-whitespace": false
  },
  "rulesDirectory": []
}
